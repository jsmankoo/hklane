        script
            :coffee-script
              $(document).ready ()->
                  #Chevron-toggle
                  $('.navbar-nav .dropdown-toggle').click ()->
                    $(this).find('i').toggleClass 'fa-chevron-up fa-chevron-down'
                  #navbar-toggle
                  $('.navbar-header .navbar-toggle').click ()->
                    $(this).find('i').toggleClass 'fa-times fa-bars'

                  #dropdownDelay
                  $('ul.nav li.dropdown').hover(
                    ()->
                      $(this).find('.dropdown-menu').stop(true, true).delay(200).fadeIn() if $(window).width() > 992
                    ()->
                      $(this).find('.dropdown-menu').stop(true, true).delay(200).fadeOut() if $(window).width() > 992
                  )

                  #navigateUp
                  $(document).scroll ()->
                    y = $(this).scrollTop();
                    if(y > 1080)
                      $('#navigateUp').fadeIn()
                    else
                      $('#navigateUp').fadeOut()

